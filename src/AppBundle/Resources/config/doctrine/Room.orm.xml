<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Room" table="room">
    <indexes>
      <index name="fk_room_house1_idx" columns="house_idhouse"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="idroom_UNIQUE" columns="idroom"/>
    </unique-constraints>
    <id name="idroom" type="integer" column="idroom">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="door" type="boolean" column="door" nullable="true"/>
    <field name="light" type="boolean" column="light" nullable="true"/>
    <many-to-one field="househouse" target-entity="House" fetch="LAZY">
      <join-columns>
        <join-column name="house_idhouse" referenced-column-name="idhouse"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
